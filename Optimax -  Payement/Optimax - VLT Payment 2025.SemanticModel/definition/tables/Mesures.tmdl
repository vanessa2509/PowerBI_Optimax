	measure MsCurrentMonthMaj =
			
			VAR MonthName = FORMAT(NOW(), "MMMM")
			VAR FirstLetter = UPPER(LEFT(MonthName, 1))
			VAR RestOfMonth = MID(MonthName, 2, LEN(MonthName) - 1)
			RETURN
			    FirstLetter & RestOfMonth & " - " & FORMAT(NOW(), "YYYY")
