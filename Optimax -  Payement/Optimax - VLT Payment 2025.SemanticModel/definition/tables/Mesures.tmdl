	measure 'Count Distinct active Agents with NEGATIEVE or NULL WLT' =
			
			VAR Result =
			    CALCULATE(
			        DISTINCTCOUNT(VLT_DIM_EMPLOYEE[FULL_NAME]),
			        FILTER(
			            VLT_DIM_EMPLOYEE,
			            VLT_DIM_EMPLOYEE[HR_STATUS] = "ACTIVE" &&
			            VLT_DIM_EMPLOYEE[HR_NUMBER] IN
			                CALCULATETABLE(
			                    VALUES(FACT_WLT_PAYMENT[EMPLID]),
			                    FACT_WLT_PAYMENT[WLT] <= 0 &&
			                    FACT_WLT_PAYMENT[MASTER_REORG_ID] IN VALUES(VLT_DIM_REORGANIZATION[MASTER_REORG_ID])
			                )
			        )
